                  <in_ref ref="DESCRIMINATOR_COADD"/>
                  <function name="rvalue"/>
                  <in_ref ref="FLOAT_DATA"/>
                  <in_ref ref="GROUP"/>
                  <in_ref ref="B_SOURCE_CAL_MERGED_COADD"/>
