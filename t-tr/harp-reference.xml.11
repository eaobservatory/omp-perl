                  <in_ref ref="TIME"/>
                  <in_ref ref="SOURCE_BASELINE"/>
                  <function name="next"/>
                  <in_ref ref="FLOAT_DATA"/>
                  <function name="rvalue"/>
                  <function name="subtract"/>
                  <in_ref ref="FLOAT_DATA"/>
                  <function name="lvalue"/>
                  <in_ref ref="FIT_RMS"/>
                  <in_ref ref="TIME"/>
                  <in_ref ref="SOURCE_BASELINE"/>
                  <function name="next"/>
                  <in_ref ref="FIT_RMS"/>
                  <function name="lvalue"/>
               </ro_recipe>
            </reducer>

